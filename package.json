{
  "name": "gemini-field-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run server:build",
    "start": "node dist-server/index.js",
    "preview": "vite preview",
    "server:dev": "tsx watch server/index.ts",
    "server:build": "tsc -p tsconfig.server.json",
    "db:init": "node scripts/init-database.js",
    "db:init:railway": "railway run node scripts/init-database.js",
    "db:seed:insurance": "node scripts/seed-insurance-companies.js",
    "db:seed:railway:insurance": "railway run node scripts/seed-insurance-companies.js",
    "admin:list-users": "node scripts/list-users.js",
    "admin:list-users:railway": "railway run node scripts/list-users.js",
    "admin:set-admin": "node scripts/set-user-admin.js",
    "admin:set-admin:railway": "railway run node scripts/set-user-admin.js",
    "test:email": "node scripts/test-email-notifications.js",
    "test:chat": "tsx scripts/test-chat-save.ts",
    "test:chat:railway": "railway run tsx scripts/test-chat-save.ts",
    "db:migrate": "node run-migrations.js",
    "db:migrate:railway": "railway run node run-migrations.js",
    "db:migrate:performance": "node run-migration-007.js",
    "db:migrate:performance:railway": "railway run node run-migration-007.js",
    "db:migrate:revenue-goals": "node run-migration-041.js",
    "db:migrate:revenue-goals:railway": "railway run node run-migration-041.js",
    "db:migrate:goals": "node run-migration-goals.js",
    "db:migrate:goals:railway": "railway run node run-migration-goals.js",
    "db:migrate:sms": "node run-migration-047.js",
    "db:migrate:sms:railway": "railway run node run-migration-047.js",
    "db:verify:indexes": "node verify-performance-indexes.js",
    "db:verify:indexes:railway": "railway run node verify-performance-indexes.js",
    "db:analyze": "psql $DATABASE_URL -c 'ANALYZE;'",
    "db:index-stats": "psql $DATABASE_URL -c \"SELECT schemaname, tablename, indexname, idx_scan as times_used, pg_size_pretty(pg_relation_size(indexrelid)) as size FROM pg_stat_user_indexes WHERE schemaname = 'public' AND indexname LIKE 'idx_%' ORDER BY idx_scan DESC;\"",
    "ios:build": "npm run build && npx cap sync ios",
    "ios:open": "npx cap open ios",
    "ios:sync": "npx cap sync ios",
    "ios:run": "npx cap run ios"
  },
  "dependencies": {
    "@assistant-ui/react": "^0.12.10",
    "@assistant-ui/react-markdown": "^0.12.3",
    "@capacitor/cli": "^7.4.5",
    "@capacitor/core": "^8.0.1",
    "@capacitor/ios": "^8.0.1",
    "@capacitor/push-notifications": "^8.0.0",
    "@docuseal/react": "^1.0.71",
    "@google/genai": "^1.27.0",
    "@ricky0123/vad-web": "^0.0.30",
    "@types/leaflet": "^1.9.21",
    "@types/multer": "^2.0.0",
    "@types/pdfkit": "^0.17.4",
    "audio-recorder-polyfill": "^0.4.1",
    "canvas": "^3.2.1",
    "carbone": "^3.5.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.6.0",
    "framer-motion": "^12.23.24",
    "googleapis": "^171.0.0",
    "heic2any": "^0.0.4",
    "helmet": "^8.1.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.548.0",
    "mammoth": "^1.6.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "pdfjs-dist": "^4.6.82",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "qr-code-styling": "^1.9.2",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^5.0.0",
    "react-markdown": "^10.1.0",
    "recharts": "^2.15.4",
    "resend": "^6.4.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "tailwind-merge": "^3.3.1",
    "twilio": "^5.12.0",
    "victory-vendor": "^37.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.36.1",
    "tailwindcss": "^4.1.16",
    "tsx": "^4.7.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.15"
  }
}
